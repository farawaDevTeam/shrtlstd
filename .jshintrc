{
  "camelcase"     : true,
  "indent"        : 4,
  "quotmark"      : "single",
  "trailing"      : true,
  "boss"          : true,
  "esnext"        : true,
  "expr"          : true,
  "proto"         : true,
  "sub"           : true,
  "validthis"     : true,
  "expr"          : true,
  "undef"         : true,     // true: Require all non-global variables to be declared (prevents global leaks)
  "unused"        : true,
  "strict"        : true,
  "multistr"      : true,
  "predef"        : [
    "console",
    "angular",
    "navigator"
  ]

}